<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.financeapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@android:style/Widget.ListPopupWindow">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name=".M"/>
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".searchStock">
            <intent-filter>
                <action android:name=".G"/>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity android:name=".findstock">
            <intent-filter>
                <action android:name="intentFindStock" />
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>

        <activity android:name=".originalList">
            <intent-filter>
                <action android:name="O" />
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>

        <activity android:name=".favorites">
        <intent-filter>
            <action android:name="F" />
            <category android:name="android.intent.category.DEFAULT"/>
        </intent-filter>
    </activity>

    </application>
</manifest>
